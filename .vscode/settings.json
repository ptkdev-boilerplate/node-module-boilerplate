{
	"[css]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[javascriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[json]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[scss]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[svelte]": {
		"editor.defaultFormatter": "svelte.svelte-vscode"
	},
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"auto-close-tag.disableOnLanguage": ["typescript", "javascript"],
	"color-highlight.matchWords": true,
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": true
	},
	"editor.cursorSmoothCaretAnimation": true,
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"editor.inlineSuggest.enabled": true,
	"editor.largeFileOptimizations": false,
	"editor.suggestSelection": "first",
	"editor.unicodeHighlight.invisibleCharacters": false,
	"editor.wordWrapColumn": 120,
	"eslint.format.enable": true,
	"eslint.options": {
		"extensinons": [".ts", ".js", ".tsx", ".jsx", ".svelte"]
	},
	"eslint.validate": ["javascript", "javascriptreact", "typescript", "typescriptreact", "svelte"],
	"files.associations": {
		".all-contributorsrc": "json",
		".all-shieldsrc": "json",
		"*.js.tpl": "typescript"
	},
	"files.autoSave": "afterDelay",
	"files.autoSaveDelay": 3000,
	"files.eol": "\n",
	"files.trimFinalNewlines": true,
	"files.watcherExclude": {
		"**/logs/**": true
	},
	"git.autofetch": true,
	"javascript.preferences.importModuleSpecifierEnding": "js",
	"material-icon-theme.folders.associations": {
		"cordova": "Vm",
		"desktop": "Container",
		"dist-client": "Dist",
		"dist-desktop": "Dist",
		"dist-mobile": "Dist",
		"drivers": "Database",
		"interface": "Include",
		"interfaces": "Include",
		"ISSUE_TEMPLATE": "Template",
		"logger": "Log",
		"mode": "Class",
		"modules": "App",
		"types": "Typescript",
		"webcomponent": "Middleware",
		"webcomponents": "Middleware",
		"wordpress": "wordpress"
	},
	"polacode.transparentBackground": true,
	"search.exclude": {
		"**/logs": true
	},
	"security.workspace.trust.enabled": false,
	"svelte.enable-ts-plugin": true,
	"terminal.integrated.persistentSessionReviveProcess": "never",
	"todo-tree.general.tags": ["BUG", "HACK", "FIXME", "TODO", "[ ]", "[x]"],
	"todo-tree.highlights.customHighlight": {
		"BUG": {
			"background": "#AC4142",
			"foreground": "white",
			"gutterIcon": true,
			"icon": "bug",
			"iconColour": "#AC4142"
		},
		"FIXME": {
			"background": "#FFCC00",
			"foreground": "black",
			"gutterIcon": true,
			"icon": "alert",
			"iconColour": "#FFCC00"
		},
		"TODO": {
			"background": "#82AAFF",
			"foreground": "black",
			"gutterIcon": true,
			"icon": "tag",
			"iconColour": "#82AAFF"
		}
	},
	"todo-tree.highlights.defaultHighlight": {
		"type": "text-and-comment"
	},
	"todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
	"typescript.preferences.importModuleSpecifierEnding": "js"
}
